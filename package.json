{
  "name": "seedz-farm-core",
  "version": "1.0.0",
  "description": "A simple library to search for the city's CEP with coordinates and IBGE Code by CEP.",
  "main": "lib/src/index.js",
  "scripts": {
    "start:dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "clean": "rimraf lib",
    "build": "npm run clean && tsc && tsc-alias",
    "test": "jest",
    "test:cov": "jest --coverage --config ./jest.config.js",
    "test:unit": "jest --runInBand --config ./jest.config.js",
    "prepublish": "npm run && npm run build",
    "lint:check": "eslint \"{src,test,utils}/**/*.ts\"",
    "lint:write": "eslint \"{src,test,utils}/**/*.ts\" --fix",
    "type:check": "./node_modules/.bin/tsc -p . --noEmit --skipLibCheck"
  },
  "keywords": [],
  "author": "Seedz",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/jest": "29.5.8",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.54.1",
    "axios-mock-adapter": "1.22.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "jest": "29.7.0",
    "prettier": "2.3.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.0.0",
    "tsc-alias": "1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "axios": "1.5.1",
    "dotenv": "16.3.1",
    "inversify": "6.0.2",
    "reflect-metadata": "0.1.13"
  }
}
